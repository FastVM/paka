 
def util_ast_is_literal(ty) {
    if ty == vm.ast.num {
        return true
    }
    if ty == vm.ast.true {
        return true
    }
    if ty == vm.ast.false {
        return true
    }
    if ty == vm.ast.none {
        return true
    }
    if ty == vm.ast.str {
        return true
    }
    if ty == vm.ast.attrs {
        return true
    }
    return false
}
