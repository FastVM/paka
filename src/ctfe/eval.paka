def ctfe_exec_do(form, state) {
    cur = none
    i = 1
    while i < length(form) {
        if state[2] != ctfe.state.run {
            return cur
        }
        cur = ctfe_exec_real(form[i], state)
        i = i + 1
    }
    return cur
}

def ctfe_exec_putchar(form, state) {
    val = ctfe_exec_real(form[1], state)
    if type(val) != type(0) {
        puts("ctfe: attempt to putchar a non-number")
    }
    putchar(val)
    return none
}

def ctfe_exec_real(form, state) {
    ty = form[0]
    if ty == vm.ast.num {
        return form[1]
    }
    if ty == vm.ast.true {
        return true
    }
    if ty == vm.ast.false {
        return false
    }
    if ty == vm.ast.none {
        return none
    }
    if ty == vm.ast.str {
        return form[1]
    }
    if ty == vm.ast.ident {
        return ctfe_exec_ident(form, state)
    }
    if ty == vm.ast.index {
        return ctfe_exec_index(form, state)
    }
    if ty == vm.ast.def {
        return none
    }
    if ty == vm.ast.return {
        state[2] = ctfe.state.ret
        state[4] = ctfe_exec_real(form[1], state)
        return none
    }
    if ty == vm.ast.macro {
        return none
    }
    if ty == vm.ast.exit {
        puts("cannot `exit` in static {...}")
        exit
    }
    if ty == vm.ast.call {
        return ctfe_exec_call(form, state)
    }
    if ty == vm.ast.putchar {
        return ctfe_exec_putchar(form, state)
    }
    if ty == vm.ast.exec {
        puts("cannot `exec(...)` in static {...}")
        exit
    }
    if ty == vm.ast.length {
        return ctfe_exec_length(form, state)
    }
    if ty == vm.ast.dump {
        puts("cannot `dump(...)` in static {...}")
        exit
    }
    if ty == vm.ast.readc {
        puts("cannot `readc(...)` in static {...}")
        exit
    }
    if ty == vm.ast.read {
        puts("cannot `read(...)` in static {...}")
        exit
    }
    if ty == vm.ast.write {
        puts("cannot `write(...)` in static {...}")
        exit
    }
    if ty == vm.ast.type {
        return ctfe_exec_type(form, state)
    }
    if ty == vm.ast.array {
        return ctfe_exec_array(form, state)
    }
    if ty == vm.ast.while {
        return ctfe_exec_while(form, state)
    }
    if ty == vm.ast.if {
        return ctfe_exec_if(form, state)
    }
    if ty == vm.ast.do {
        return ctfe_exec_do(form, state)
    }
    if ty == vm.ast.static {
        return ctfe_exec_real(form[1], state)
    }
    if ty == "=" {
        return ctfe_exec_set(form, state)
    }
    if ty == "~=" {
        return ctfe_exec_extend(form, state)
    }
    if ty == "~" {
        return ctfe_exec_concat(form, state)
    }
    if ty == "+" {
        return ctfe_exec_add(form, state)
    }
    if ty == "-" {
        return ctfe_exec_sub(form, state)
    }
    if ty == "*" {
        return ctfe_exec_mul(form, state)
    }
    if ty == "/" {
        return ctfe_exec_div(form, state)
    }
    if ty == "%" {
        return ctfe_exec_mod(form, state)
    }
    if ty == "<" {
        return ctfe_exec_lt(form, state)
    }
    if ty == ">" {
        return ctfe_exec_gt(form, state)
    }
    if ty == "<=" {
        return ctfe_exec_lte(form, state)
    }
    if ty == ">=" {
        return ctfe_exec_gte(form, state)
    }
    if ty == "==" {
        return ctfe_exec_eq(form, state)
    }
    if ty == "!=" {
        return ctfe_exec_neq(form, state)
    }
    put("exec: unknown form: ")
    println(form)
    exit
}


def ctfe_exec_const(form, state) {
    return form
}

def ctfe_exec_copy(form, state) {
    ret = [form[0]]
    i = 1
    while i < length(form) {
        ret ~= [ctfe_exec(form[i], state)]
        i = i + 1
    }
    return ret
}

def ctfe_real_to_ast(val) {
    if val == none {
        return [vm.ast.none]
    }
    if val == false {
        return [vm.ast.false]
    }
    if val == true {
        return [vm.ast.true]
    }
    if type(val) == type(0) {
        return [vm.ast.num, val]
    }
    if type(val) == type([]) {
        ret = [vm.ast.array]
        i = 0
        while i < length(val) {
            ret ~= ctfe_real_to_ast(val[i])
            i = i + 1
        }
        return ret
    }
    put("cannot turn value into ast: ")
    println(val)
    exit
}

def ctfe_exec(form, state) {
    ty = form[0]
    if ty == vm.ast.num {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.true {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.false {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.none {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.str {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.ident {
        return ctfe_exec_const(form, state)
    }
    if ty == vm.ast.index {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.def {
        state[0] ~= [form[1][1], form[2]]
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.return {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.macro {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.exit {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.call {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.putchar {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.exec {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.length {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.dump {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.readc {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.read {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.write {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.type {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.array {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.while {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.if {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.do {
        return ctfe_exec_copy(form, state)
    }
    if ty == vm.ast.static {
        real = ctfe_exec_real(form[1], state)
        ast = ctfe_real_to_ast(real)
        return ast
    }
    if ty == "=" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "~=" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "~" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "+" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "-" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "*" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "/" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "%" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "<" {
        return ctfe_exec_copy(form, state)
    }
    if ty == ">" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "<=" {
        return ctfe_exec_copy(form, state)
    }
    if ty == ">=" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "==" {
        return ctfe_exec_copy(form, state)
    }
    if ty == "!=" {
        return ctfe_exec_copy(form, state)
    }
    put("eval: unknown form: ")
    println(form)
    exit
}