def ctfe_exec_ident(form, state) {
    ret = findvar(state[1], form[1])
    if ret == none {
        put("ctfe: undefined ident: ")
        puts(ret)
        exit
    }
    return ret
}

def ctfe_exec_index(form, state) {
    arr = ctfe_exec_real(form[1], state)
    ind = ctfe_exec_real(form[2], state)
    if type(arr) != type([]) {
        puts("ctfe: attempt to index non-array")
        exit
    }
    if ind < 0 {
        ind = ind + length(arr)
    }
    if ind >= length(arr) {
        puts("ctfe: index out of bounds: too high")
        exit
    }
    if ind < 0 {
        puts("ctfe: index out of bounds: too low")
        exit
    }
    ret = arr[ind]
    return ret
}

def ctfe_exec_length(form, state) {
    arr = ctfe_exec_real(form[1], state)
    if type(arr) != type([]) {
        puts("ctfe: attempt to find length of non-array")
        exit
    }
    ret = length(arr)
    return ret
}

def ctfe_exec_array(form, state) {
    arr = []
    i = 1
    while i < length(form) {
        arr ~= ctfe_exec_real(form[i], state)
        i = i + 1
    }
    return arr
}

def ctfe_exec_while(form, state) {
    while true {
        cond = ctfe_exec_real(form[1], state)
        if cond == false {
            return none
        }
        then = ctfe_exec_real(form[2], state)
        if state[2] != ctfe.state.run {
            return none
        }
    }
}

def ctfe_exec_if(form, state) {
    cond = ctfe_exec_real(form[1], state)
    if cond {
        ctfe_exec_real(form[2], state)
    } else {
        ctfe_exec_real(form[3], state)
    }
    return none
}

def ctfe_exec_set(form, state) {
    if form[1][0] == vm.ast.ident {
        vars = state[1]
        name = form[1][1]
        setto = ctfe_exec_real(form[2])
        i = 0
        while i < length(vars) {
            if vars[i] == name {
                vars[i + 1] = setto
            }
            i = i + 2
        }
        vars ~= [name, setto]
        return setto
    }
    if form[1][0] == vm.ast.index {
        arr = ctfe_exec_real(form[1][1])
        ind = ctfe_exec_real(form[1][2])
        val = ctfe_exec_real(form[2])
        if type(arr) != type([]) {
            puts("ctfe: attempt to index non-array")
            exit
        }
        if ind < 0 {
            ind = ind + length(arr)
        }
        if ind >= length(arr) {
            puts("ctfe: index out of bounds: too high")
            exit
        }
        if ind < 0 {
            puts("ctfe: index out of bounds: too low")
            exit
        }
        arr[ind] = val
    }
}

def ctfe_exec_type(form, state) {
    return type(ctfe_exec_real(form[1]))
}

def ctfe_exec_call(form, state) {
    fulldef = findvar(state[0], form[1][1])
    if fulldef == none {
        puts("cannot find function in ctfe: " ~ form[1][1])
        exit
    }
    nparams = length(fulldef[1]) - 2
    nargs = length(form) - 2
    if nparams > nargs {
        puts("expected more params")
        exit
    }
    if nparams < nargs {
        puts("expected less params")
        exit
    }
    vars = []
    i = 2
    while i < length(fulldef[1]) {
        res = ctfe_exec_real(form[i], state)
        vars ~= [fulldef[1][i][1], res]
        i = i + 1
    }
    nstate = [state[0], vars, ctfe.state.run, state[3], none]
    ctfe_exec_real(fulldef[2], nstate)
    return nstate[4]
}

def ctfe_exec_extend(form, state) {
    lhs = ctfe_exec_real(form[1], state)
    rhs = ctfe_exec_real(form[2], state)
    if type(lhs) != type([]) {
        puts("ctfe: attemt to extend non-array")
        exit
    }
    if type(rhs) != type([]) {
        puts("ctfe: attemt to extend array with non-array")
        exit
    }
    lhs ~= rhs
    return lhs
}

