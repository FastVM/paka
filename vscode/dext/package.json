{
    "name": "dext",
    "displayName": "Dext",
    "description": "Dext Languag Support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "dext",
            "aliases": ["dext"],
            "extensions": [".dext",".d9c"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "dext",
            "scopeName": "lang",
            "path": "./syntaxes/dext.tmLanguage.json"
        }]
    }
}