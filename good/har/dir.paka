--- main.paka
parse_csv = sys.import("parse_csv.paka");
print2d = sys.import("print2d.paka");

csv_data = io.slurp("input.csv");
data = parse_csv(csv_data);
print2d(data);

--- parse_csv.paka
def parse(raw_csv) {
    clean_csv = str.strip(raw_csv);
    lines = str.split(clean_csv, "\n");
    data = arr.map(lines) {
        str.split($0, ",");
    };
    return data;
};
return parse;

--- print2d.paka
def print2d(array) {
    io.print("[");
    arr.each(array) {
        io.put("    [");
        arr.each($0) {
            if ($1 != 0) {
                io.put(", ");
            };
            io.put($0);
        };
        io.print("]");
    };
    io.print("]");
};
return print2d;

