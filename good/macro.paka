include std/bind;

def array_starting_at(n; ) {
    a = args;
    i = n;
    res = []
    while (i < arr.len(a) + n) {
        index = i - n
        arr.push(res, => a(index))
        i += 1
    }
    return => res($0;)
}

array_from_one = array_starting_at(1)

1to5 = array_from_one(1, 2, 3, 4, 5)
io.print(1to5(3) == 3)