def add1(x)(y) {
    x + y
};

def add2(x; y) {
    x + y
};

add3(x)(y) = x + y;

add4(x; y) = x + y;

adders = [add1, add2, add3, add4];

arr.each (adders) {
    add = $0;
    inc = add(1);
    dec = add(-1);
    assert inc(13) == 14;
    assert dec(11) == 10;
    assert add(7; 5) == 12;
    assert add(3)(2) == 5;
    io.print(add);
};
