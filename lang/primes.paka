max := 100
total := 0
if max >= 2 {
    total = 1
}
sum := 0
cur := 3
while cur < max {
    test := 3
    is_prime := true
    while test * test <= cur {
        if cur % test == 0 {
            is_prime = false   
        }
        test = test + 2
    }
    if is_prime {
        total = total + 1 
    }
    cur = cur + 2
}
println(total)