def check(num) {
    test := 3
    while test * test <= num {
        if num % test == 0 {
            return false
        }
        test = test + 2
    }
    return true
}

def main() {
    cur := 3
    count := 1
    while cur <= 10000000 {
        if check(cur) {
            count = count + 1
        }
        cur = cur + 2
    }
    println(count)
}

main()
