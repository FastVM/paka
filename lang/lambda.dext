n = 0
res = 0
while (n < 10) {
    x = table {
        .val = 1
        .set = lambda(val) {
            .val = val
            return .val
        }
    }
    res -= x.set(n)

    y = table {
        .val = 300
        .set = lambda(self, val) {
            self.val = val
            return .val
        }
    }
    n += 1
    res += y::set(n)
}
print(res == 10)