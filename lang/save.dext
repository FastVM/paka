def fib(x) {
    if (x < 2) {
        return x
    }
    return fib(x-2) + fib(x-1)
}

fibv = fib(5)
def run() {
    if(json.dumpf("world/saved.json")) {
        io.print("Loading World!")
    } else {
        io.print("Saving World!")
    }
    io.print(fib(5))
    io.print(fibv)
}
run()
io.print("Done!")
