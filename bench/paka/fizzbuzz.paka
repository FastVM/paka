def one_fizzbuzz(n) {
    res = ""
    if n % 3 == 0 {
        res = res ~ "Fizz"
    } 
    if n % 5 == 0 {
        res = res ~ "Buzz"
    }
    if n % 7 == 0 {
        res = res ~ "Woof"
    }
    if res == "" {
        str.from(n)
    } else {
        res
    }
}

def fizzbuzz_count(c) {
    count = 0
    i = 0
    while i < c {
        count = count + str.len(one_fizzbuzz(i))
        i = i + 1
    }
    count
}

fizzbuzz_count(10000000) |> io.print